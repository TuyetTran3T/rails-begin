= form_with(model: ais_vessal_destination, local: true) do |form|
  - if ais_vessal_destination.errors.any?
    #error_explanation
      %h2
        = pluralize(ais_vessal_destination.errors.count, "error")
        prohibited this ais_vessal_destination from being saved:
      %ul
        - ais_vessal_destination.errors.full_messages.each do |message|
          %li= message
  .field
    = form.label :vessel_id
    = form.number_field :vessel_id, id: :ais_vessal_destination_vessel_id
  .field
    = form.label :destination
    = form.text_field :destination, id: :ais_vessal_destination_destination
  .field
    = form.label :draught
    = form.text_field :draught, id: :ais_vessal_destination_draught
  .field
    = form.label :eta
    = form.datetime_select :eta, id: :ais_vessal_destination_eta
  .actions
    = form.submit
